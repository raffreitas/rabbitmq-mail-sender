{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:prettier/recommended",
    "plugin:vitest-globals/recommended",
    "prettier",
    "eslint:recommended"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "vitest-globals/env": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ]
  }
}
